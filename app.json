{
  "expo": {
    "name": "React Native Demo Wallet",
    "slug": "react-native-demo-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "react-native-demo-wallet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.tkhqlabs.passkeyapp",
      "associatedDomains": [
        "webcredentials:passkeyapp.tkhqlabs.xyz"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.776352896366-vscu7dt8umrlihuv8g54laphblm2rsbm"
            ]
          }
        ]
      },
      "appleTeamId": "74V7N6MW3W"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "xyz.tkhqlabs.passkeyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
